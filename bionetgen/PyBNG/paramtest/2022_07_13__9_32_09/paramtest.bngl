begin model
begin parameters
  T_END 100
end parameters
begin species
  S() 99
  I() 1
end species
begin observables
  Molecules S S()
  Molecules I I()
  Molecules R R()
end observables
begin reaction rules
  R1: S() + I() -> I() + I() 0.02 
  R2: I() -> R() 1
end reaction rules
end model
simulate({method=>"ode",t_end=>"T_END",n_steps=>100})